<Window x:Class="NewTrackerforMike.View.WarningView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ignore="http://www.galasoft.ch/ignore"
        xmlns:vm="clr-namespace:NewTrackerforMike.ViewModel"
        mc:Ignorable="d ignore"
        DataContext="{Binding WarningVM, Source={StaticResource Locator}}" Background="{DynamicResource WindowBackgroundColor}" Foreground="{DynamicResource LabelTextColor}" WindowStartupLocation="CenterScreen">

    <Window.Resources>
        <vm:ViewModelLocator x:Key="vmLocator" />
    </Window.Resources>

    <Grid>
        <DataGrid x:Name="dataGrid" Background="{x:Null}" Foreground="{DynamicResource LabelTextColor}" 
                  ItemsSource="{Binding LowMedList, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
    </Grid>
</Window>
